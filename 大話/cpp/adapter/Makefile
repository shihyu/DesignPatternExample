adapter.pro:
qmake: FORCE
	@$(QMAKE) -o Makefile adapter.pro

qmake_all: FORCE

dist: 
	@test -d .tmp/adapter1.0.0 || mkdir -p .tmp/adapter1.0.0
	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/adapter1.0.0/ && (cd `dirname .tmp/adapter1.0.0` && $(TAR) adapter1.0.0.tar adapter1.0.0 && $(COMPRESS) adapter1.0.0.tar) && $(MOVE) `dirname .tmp/adapter1.0.0`/adapter1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/adapter1.0.0


clean:compiler_clean 
	-$(DEL_FILE) $(OBJECTS)
	-$(DEL_FILE) *~ core *.core


####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) 
	-$(DEL_FILE) Makefile


compiler_clean: 

####### Compile

AdapterMain.o: AdapterMain.cpp AdapterMain.h \
		Guard.h \
		Player.h \
		Translator.h \
		Center.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o AdapterMain.o AdapterMain.cpp

Center.o: Center.cpp Center.h \
		Player.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o Center.o Center.cpp

Guard.o: Guard.cpp Guard.h \
		Player.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o Guard.o Guard.cpp

Player.o: Player.cpp Player.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o Player.o Player.cpp

Translator.o: Translator.cpp Translator.h \
		Player.h \
		Center.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o Translator.o Translator.cpp

####### Install

install:   FORCE

uninstall:   FORCE

FORCE:

